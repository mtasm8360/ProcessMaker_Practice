        <tri_title>04_CHECK_NATIONAL_CODE</tri_title>
        <tri_description></tri_description>
        <pro_uid>51151722066715f4455f309011505833</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$act = $_POST['act'];
if($act=='CHECK_NATIONAL_CODE'){
	$code = $_POST['code'];
	$query = "SELECT USR_UID FROM USERS WHERE USR_USERNAME='$code'";
	
	$result = executeQuery($query);
	header('Content-type: application/json');
	 
	if($result){
		echo json_encode("user exists!");
		die();
		
	}else{
		echo json_encode("user dosnt exist.");
		die();
	}
}]]></tri_webbot>
        <tri_param></tri_param>
